---
import Error404 from "@/components/infopages/Error404.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "404 Error: Página no encontrada - solucion.io",
	description:
		"¡Ups! La página que buscas no existe. Vuelve a la página de inicio de solucion.io para conocer nuestra gama completa de soluciones tecnológicas.",
